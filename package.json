{
  "name": "gatsby-source-fitbit",
  "version": "1.0.0",
  "description": "Gatsby source plugin for using data from Fitbit on your website",
  "main": "index.js",
  "bin": "token-tool.js",
  "author": "Kyle Jones <kylejones1310@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kerl1310/gatsby-source-fitbit.git"
  },
  "homepage": "https://github.com/Kerl1310/gatsby-source-fitbit",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "fitbit"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "lint": "npm-run-all --continue-on-error -p lint:code lint:other",
    "lint:code": "tslint --project tsconfig.json",
    "lint:other": "npm run prettier -- --check",
    "format": "npm run format:code && npm run format:other",
    "format:other": "npm run prettier -- --write",
    "format:code": "npm run lint:code -- --fix",
    "release": "release-it"
  },
  "files": [
    "*.js",
    "*.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.6.9",
    "@types/node-fetch": "^2.5.0",
    "@types/open": "^6.2.1",
    "@types/prettier": "^1.18.1",
    "@release-it/conventional-changelog": "^1.1.0",
    "acorn": "^7.2.0",
    "commander": "^2.20.0",
    "dependencies": "0.0.1",
    "gatsby": "^2.22.15",
    "gatsby-node-helpers": "^0.3.0",
    "gatsby-source-filesystem": "^2.3.8",
    "inquirer": "^6.5.0",
    "node-fetch": "^2.6.0",
    "open": "^6.4.0",
    "prettier": "^1.19.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.9.3",
    "release-it": "^12.4.3",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    
  },
  "peerDependencies": {
    "gatsby": "^2.10.8"
  }
}
